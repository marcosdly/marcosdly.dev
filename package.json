{
  "name": "marcosdly.dev",
  "type": "module",
  "private": true,
  "version": "0.0.0",
  "description": "A hub for my work, a website 😐",
  "scripts": {
    "dev": "vite -m dev",
    "build": "tsc && vite build -m dev",
    "build:prod": "tsc && vite build -m prod",
    "test": "vitest"
  },
  "keywords": [
    "dev",
    "portfolio",
    "website"
  ],
  "author": "Marcos Gabriel <dev.marcosdly@gmail.com>",
  "license": "MPL-2.0",
  "homepage": "https://marcosdly.dev",
  "cdnAlias": {
    "regex": {
      "preact": {
        "expr": "preact(\\/.*)?",
        "path": "dist/preact.min.umd.js"
      }
    },
    "replace": {
      "highlight.js": "@highlightjs/cdn-assets"
    },
    "npm": {
      "marked": "marked.min.js",
      "marked-base-url": "lib/index.umd.js",
      "marked-footnote": "dist/index.umd.min.js",
      "marked-extended-tables": "lib/index.umd.js",
      "marked-highlight": "lib/index.umd.js",
      "marked-smartypants-lite": "lib/index.umd.js",
      "marked-directive": "dist/index.umd.js",
      "dompurify": "dist/purify.min.js",
      "@highlightjs/cdn-assets": "highlight.min.js"
    }
  },
  "devDependencies": {
    "@preact/preset-vite": "2.8.1",
    "@prettier/plugin-xml": "3.2.2",
    "@types/node": "20.11.24",
    "hex-to-css-filter": "5.4.0",
    "prettier": "3.2.5",
    "prettier-plugin-jsdoc": "1.3.0",
    "prettier-plugin-organize-imports": "3.2.4",
    "sass": "1.70.0",
    "typescript": "5.2.2",
    "vite": "5.1.1",
    "vitest": "1.3.0"
  },
  "dependencies": {
    "dompurify": "3.0.8",
    "highlight.js": "11.9.0",
    "marked": "11.2.0",
    "marked-base-url": "1.1.3",
    "marked-directive": "1.0.5",
    "marked-extended-tables": "1.0.8",
    "marked-footnote": "1.2.2",
    "marked-highlight": "2.1.1",
    "marked-smartypants-lite": "1.0.2",
    "preact": "10.19.6"
  }
}
